# PROVIDE: snmpd
# REQUIRE: NETWORKING
# KEYWORD: nojail

snmpd_enable=${snmpd_enable-"NO"}

. /etc/rc.subr
. /etc/conf.subr

name="snmpd"
rcvar=`set_rcvar`
snmpd_config=${snmpd_config:-"/var/etc/${name}.conf"}
required_files=${snmpd_config}
pidfile="/var/run/${name}.pid"
command="/usr/sbin/${name}"
command_args="-c ${snmpd_config} -P ${pidfile}"
start_precmd="start_precmd"

start_precmd()
{
	# Create .conf file
	create_conf_file ${name}
}

load_rc_config ${name}
run_rc_command "$1"
