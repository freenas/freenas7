# PROVIDE: inadyn
# REQUIRE: DAEMON
# KEYWORD: shutdown
# XPATH: //dynamicdns/enable

inadyn_enable=${inadyn_enable:-"NO"}

. /etc/rc.subr
. /etc/conf.subr

name="inadyn"
rcvar=`set_rcvar`
inadyn_config=${inadyn_config:-"/var/etc/${name}.conf"}
required_files=${inadyn_config}
command="/usr/local/bin/${name}"
command_args="--input_file ${inadyn_config}"

load_rc_config ${name}
create_conf_file ${name}
run_rc_command "$1"
