diff -ruN /sys/conf/files.amd64.orig /sys/conf/files.amd64 >> patch-files.diff

diff -ruN /sys/dev/mii/ciphy.c.orig /sys/dev/mii/ciphy.c > patch-phy.diff
diff -ruN /sys/dev/mii/ciphyreg.h.orig /sys/dev/mii/ciphyreg.h >> patch-phy.diff
