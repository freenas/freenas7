# PROVIDE: afpd
# REQUIRE: DAEMON 
# KEYWORD: shutdown

afpd_enable=${afpd_enable-"NO"}

. /etc/rc.subr
. /etc/conf.subr

name="afpd"
rcvar=`set_rcvar`
afpd_config=${afpd_config:-"/var/etc/${name}.conf"}
required_files=${afpd_config}
command="/usr/local/sbin/${name}"
command_args="-F ${afpd_config}"
start_precmd="start_precmd"

start_precmd()
{
	# Create .conf file
	create_conf_file ${name}
}

load_rc_config ${name}
run_rc_command "$1"
