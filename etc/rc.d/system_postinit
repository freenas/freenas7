# PROVIDE: system_postinit

. /etc/rc.subr

name="system_postinit"
stop_cmd=":"
start_cmd="start_cmd"

start_cmd()
{
	# Start core system.
	eval /etc/rc.d.php/${name}
	return $?
}

load_rc_config ${name}
run_rc_command "$1"
