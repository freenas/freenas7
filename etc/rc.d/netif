# PROVIDE: netif
# REQUIRE: mountcritlocal pccard sysctl
# KEYWORD: nojail

. /etc/rc.subr

name="network"
start_cmd="start_cmd"
stop_cmd=":"

start_cmd()
{
	local _result
	# Bring up network interfaces.
	eval /etc/rc.d.php/netif
	_result=$?

	# Display interfaces configured by this script
	/sbin/ifconfig

	return ${_result}
}

load_rc_config ${name}
run_rc_command "$1"
